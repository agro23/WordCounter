@{
  Layout = "_Layout";
}
  <div class="container well">
    <h1>Rock Paper Scissors</h1>
    <div class="row">
      @if(@Model["msg"].Length > 0)
      {
        <h3>@Model["msg"]</h3>
      }
    </div>
    <div class="row">
      <form class="form" action="/" method="get">
        <button type="submit" class="btn btn-success" name="button">DO ANOTHER?</button>
      </form>
    </div>
  </div>
